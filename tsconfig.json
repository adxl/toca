{
  "compilerOptions": {
    "target": "es2022",
    "jsx": "react",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowJs": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "types": ["node"],
    "baseUrl": ".",
    "declaration": true,
    "outDir": "dist",
    "rootDir": "."
  }
}
